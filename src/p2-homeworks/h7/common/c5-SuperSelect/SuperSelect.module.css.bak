
.select {
    width: 200px;
    padding: 5px;
    padding-right: 30px;
    /* height: 100px; */
    cursor: pointer;
    background-color: #93c3dd;
    background-image: linear-gradient(45deg, #93c3dd 0%, #33d8de96 50%, #133d92c8 100%);
   margin: 1px;
    box-shadow: inset 0 0 0 0.125em #844685;
    border-radius: 10px;
    font-size: large;
}